                                <div class="areaHeader" style="display:flex;">
                                    <div id="tasksLoader" class="pull-left loader-inline" style="display:none"></div>
                                    <div class="areaHeaderTitle">
                                        <span id="areHeadertitle" class="areaHeaderTitleNode menu-text">Tasks</span>
                                    </div>
                                    <div class="pull-right text-right p-r-20 areaHeaderBtnSec">
                                        <a id="addTaskBtn" onclick="areaHeaderBtnClick(event);" class="btn btn-outline-dark areaHeaderBtn"><i class="fa fa-plus fa-fw"></i> Add New</a>
                                    </div>
                                </div>
                                <div role="form" id="tasksForm" class="row areaForm" method="post" data-toggle="validator" style="display: none; margin:0; padding:20px; box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.05), 0 0px 10px 0px rgba(0, 0, 0, 0.05);">
                                    <div class="row">
                                        <div class="col-lg-9 col-md-9 col-sm-9 col-12">
                                            <div class="form-group">
                                                <label class="margin-bottom-5 control-label">Tasks:</label>
                                                <select id="tasksIds" name="TasksIds" multiple="multiple" class="form-control">
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-12">
                                            <label class="control-label m-b-5 "></label>
                                            <button id="submitTaskBtn" type="button" class="btn btn-success btn-block">Add Task(s)</button>
                                        </div>
                                    </div>
                                </div>
